    <value>local LogicAPI = require("LogicAPI")


local root = LogicAPI.Root

local function Create_Pine(logicmanager)
    local v1 = logicmanager.Create_Negate()
    local v2 = logicmanager.Create_Negate()
    v1.AddOut(v2)
    return {
        AddOut = function (...)
            v2.AddOut(...)
        end,
        AddIn = function (...)
            v1.AddIn(...)
        end,
        inputlogic = v1,
        outputlogic = v2
    }
end

local function Create_Circuit_NAND(logicmanager)
    local mygroup = logicmanager.Create_Group()

    mygroup.Execute(function ()
        local and1 = logicmanager.Create_And()

        local pine1 = Create_Pine(logicmanager)
        local pine1_negate = logicmanager.Create_Negate()
        pine1.AddOut(pine1_negate)
        local pine2 = Create_Pine(logicmanager)
        local pine2_negate = logicmanager.Create_Negate()
        pine2.AddOut(pine2_negate)
        local pine3 = Create_Pine(logicmanager)

        and1.AddIn(pine1_negate,pine2_negate)
        pine3.AddIn(and1)

        pine1.inputlogic.SetPosition(LogicAPI.Vector3.new(-1,10,0.5))
        pine2.inputlogic.SetPosition(LogicAPI.Vector3.new(-1,10,-0.5))
        pine3.outputlogic.SetPosition(LogicAPI.Vector3.new(1,10,0))
    end)
end

root.Execute(function (logicmanager)
    Create_Circuit_NAND(logicmanager)
end)</value>
